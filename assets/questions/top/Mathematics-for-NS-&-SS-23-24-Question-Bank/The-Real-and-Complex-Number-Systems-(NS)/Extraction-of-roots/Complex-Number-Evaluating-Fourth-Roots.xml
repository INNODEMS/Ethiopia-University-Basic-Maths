    <questionvariables>
      <text><![CDATA[/* For each of the 4 quadrants, there are 3 different argument values to choose from so that the complex number has nice values in both cartesian and polar form:
https://en.wikipedia.org/wiki/List_of_trigonometric_identities#/media/File:Unit_circle_angles_color.svg */
quadrant: rand(4);
angle: rand([%pi/2, %pi/3, %pi/4, %pi/6]);
arg: if -%pi<(angle+%pi/2*quadrant) and (angle+%pi/2*quadrant)<=%pi then (angle+%pi/2*quadrant) else (angle+%pi/2*quadrant)-2*%pi;
radius: rand_with_step(1, 9, 1);
zz: radius*%e^(%i*arg);

/*zz_cartesian does not always display the real part first then the imaginary part*/
z_real: realpart(zz);
z_imag: imagpart(zz);
zz_cartesian: z_real + z_imag * %i;

/* polarform(zz) does not give the complex number in the form zz (above) in fact it gives the complex number in cartesian form!*/
ta1: radius;

ta2: arg;

simp:false;
ta3: radius*%e^(ta2*%i);

ta89:radius*(cos(ta2) + %i*sin(ta2));

/*Fourth roots*/
simp: false;
argta4: if -%pi<(arg/4) and (arg/4)<=%pi then ev((arg/4),simp) else ev(((arg/4)-2*%pi),simp); 
ta4: radius^(1/4)*(cos(argta4) + %i *sin(argta4));
argta5: if -%pi<((arg+2*%pi)/4) and ((arg+2*%pi)/4)<=%pi then ev(((arg+2*%pi)/4),simp) else ev((((arg+2*%pi)/4)-2*%pi),simp); 
ta5: radius^(1/4)*(cos(argta5) + %i* sin(argta5));
argta6: if -%pi<((arg+4*%pi)/4) and ((arg+4*%pi)/4)<=%pi then ev(((arg+4*%pi)/4),simp) else ev((((arg+4*%pi)/4)-2*%pi),simp);
ta6: radius^(1/4)*(cos(argta6) + %i* sin(argta6));
argta7: if -%pi<((arg+6*%pi)/4) and ((arg+6*%pi)/4)<=%pi then ev(((arg+6*%pi)/4),simp) else ev((((arg+6*%pi)/4)-2*%pi),simp);
ta7: radius^(1/4)*(cos(argta7) + %i* sin(argta7));
teacher_roots: {ta4, ta5, ta6, ta7}; /*teacher fourth roots*/]]></text>
    </questionvariables>
