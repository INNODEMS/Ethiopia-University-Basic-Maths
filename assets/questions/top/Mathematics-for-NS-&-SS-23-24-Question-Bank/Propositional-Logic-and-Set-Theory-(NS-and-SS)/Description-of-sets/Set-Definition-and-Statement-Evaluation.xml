    <questionvariables>
      <text><![CDATA[/* Setting parameters for part (a) */

upper_bound_a: rand(5)+5;
lower_bound_a: -rand(4)-2;

talist1:makelist(i,i,lower_bound_a,upper_bound_a);
ta1:setify(talist1);

/* Setting parameters for part (b) */

upper_bound_b: rand(5)+5;
lower_bound_b: rand_with_prohib(-6,-1,[lower_bound_a]);

talist2:makelist(i,i,1,upper_bound_b);
ta2:setify(talist2);

/* Determine the correct statement of (c) */

choices: [
  [1, false, "\\(A\\) is a proper subset of \\(B\\)"], 
  [2, is(upper_bound_a >= upper_bound_b), "\\(B\\) is a proper subset of \\(A\\)"], 
  [3, false, "\\(A\\) is equal to \\(B\\)"], 
  [4, is(upper_bound_a < upper_bound_b), "\\(A\\) is not a subset of \\(B\\) and \\(B\\) is not a subset of \\(A\\)"]
];

ta3:random_permutation(choices);
tac: if upper_bound_a >= upper_bound_b then 2 else 4;

/* Answer-independent mistakes */

/* (a) */

/* N2: Correct answer given with with strings not numbers */
talistword1:makelist(string(i),i,lower_bound_a,upper_bound_a);
taword1:setify(talistword1);

/* N3: Treats bounds as strict */
part_a_n3_1: setify(makelist(i,i,lower_bound_a + 1,upper_bound_a));
part_a_n3_2: setify(makelist(i,i,lower_bound_a,upper_bound_a - 1));
part_a_n3_3: setify(makelist(i,i,lower_bound_a + 1,upper_bound_a - 1));

/* (b) */
/* N2: Correct answer given with with strings not numbers */
talistword2:makelist(string(i),i,1,upper_bound_b);
taword2:setify(talistword2);

/* N3: Includes non-positive elements (but otherwise correct) */
part_b_n3_1: setify(makelist(i,i,0,upper_bound_b));

/* N4: Treats upper bound as strict */
part_b_n4_1: setify(makelist(i,i,1,upper_bound_b-1));

/* N4-F: Otherwise incorrect */

/* (c) */

/* N2: Error carried forward */
part_c_ecf_ans1: {0};
part_c_ecf_ans2: {0};
part_c_ecf_ans3: 3;

/* N3: Gives A is a proper subset of B */
part_c_n3_1: 1;

/* N4: Gives A is equal to B */
part_c_n4_1: 3;

/* N5: Gives incorrect of two potentially correct answers */
part_c_n5_1: if tac=2 then 4 else 2;]]></text>
