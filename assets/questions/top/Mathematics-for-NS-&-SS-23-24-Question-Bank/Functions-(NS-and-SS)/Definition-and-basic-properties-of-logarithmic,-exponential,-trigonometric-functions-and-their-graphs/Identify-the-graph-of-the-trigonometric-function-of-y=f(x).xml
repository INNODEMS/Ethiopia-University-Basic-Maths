    <questionvariables>
      <text><![CDATA[aa:rand(3)+2;
bb:(rand(7)+1)*%pi/4;
fx:aa*sin(x-float(bb));
ta11:aa*sin(x-bb*180/%pi);
ta12:-aa*sin(x-180-bb*180/%pi);
ta13:aa*sin(x+360-bb*180/%pi);
ta14:-aa*sin(x+180-bb*180/%pi);
ta15:-aa*sin(x+540-bb*180/%pi);
ta21:aa*cos(x-90-bb*180/%pi);
ta22:-aa*cos(x-270-bb*180/%pi);
ta23:aa*cos(x+270-bb*180/%pi);
ta24:-aa*cos(x+90-bb*180/%pi);
ta25:-aa*cos(x+450-bb*180/%pi);
ta3:abs(aa);
minshift: if 90+bb*180/%pi>=360 then -270+bb*180/%pi else 90+bb*180/%pi;]]></text>
    </questionvariables>
