    <questionvariables>
      <text>aa:rand(8)+1;
pm1:rand_with_prohib(-1,1,[0]);
bb:rand(8)+1;
pm2:rand_with_prohib(-1,1,[0]);
cc:rand(8)+1;
fx:expand((x-pm1*aa)*(x-pm2*bb)+cc);
xv:(pm1*aa+pm2*bb)/2;
yv:ev(fx,x=xv);
ta1:oo(-inf,inf);
ta2:co(yv,inf);

/*worked solution: unevaluated lines*/
simp:false;

line1:(x - ev((pm1*aa + pm2*bb)/2,simp))^2 - ev((pm1*aa + pm2*bb)^2/4,simp) + ev(pm1*pm2*aa*bb + cc,simp);

line2:(x - ev((pm1*aa + pm2*bb)/2,simp))^2 + ev(-(pm1*aa + pm2*bb)^2/4 + pm1*pm2*aa*bb + cc,simp);

simp:true;</text>
