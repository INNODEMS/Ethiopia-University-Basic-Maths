      <text><![CDATA[vertices_hyp_1:0; 
vertices_hyp_2:rand(5)+1; 
eccentricity_hyp: (rand_with_prohib(11, 22, [0]) / 10); 
center_foci:decimalplaces(eccentricity_hyp*vertices_hyp_2,2); 
b_square:decimalplaces(vertices_hyp_2^2*((eccentricity_hyp^2)-1),2);
point_f1:eccentricity_hyp*vertices_hyp_2;
point_f2:decimalplaces(eccentricity_hyp*vertices_hyp_2,2);
point_f3:round(eccentricity_hyp*vertices_hyp_2);   
point_f4:is(abs(point_f3-point_f2)=0.0);  
point_f: if point_f4=true then point_f3 else point_f2;
point_1:matrix([0],[point_f]);
point_2:matrix([0],[-point_f]);

point_1_1:matrix([0],[point_f1]);
point_2_2:matrix([0],[-point_f1]);
x_1:rand([-10,vertices_hyp_2,-8,0,vertices_hyp_2,-vertices_hyp_2]);
y_1:rand([0,1,-1,4,-4]);
x_2:rand([-10,vertices_hyp_2,-8,0,vertices_hyp_2,-vertices_hyp_2]);
y_2:rand([0,2,-2,-3,3]);
eqn_1:(x^2/(vertices_hyp_2^2))-(y^2/b_square)=1;
eqn_2:(x^2/(vertices_hyp_2^2))-(y^2/b_square);
first_cordinate:subst(x_1,x,eqn_2);



ta_a1:subst(y_1,y,first_cordinate);
ta_a2:decimalplaces(subst(y_1,y,first_cordinate),2);
ta_a3:round(subst(y_1,y,first_cordinate));   
ta_a4:is(abs(ta_a3-ta_a2)=0.0);  
ta_1: if ta_a4=true then ta_a3 else ta_a2;


second_cordinate:subst(x_2,x,eqn_2);
ta_2:decimalplaces(subst(y_2,y,second_cordinate),2);



ta_3_b:random_permutation([["is on the hyperbola",true],["is not on the hyperbola",false]]);
ta_3_nb:random_permutation([["is on the hyperbola",false],["is not on the hyperbola",true]]);
ta_3:if ta_1=1 then ta_3_b else ta_3_nb;
ta_4:if ta_2=1 then ta_3_b else ta_3_nb;



b_square1:vertices_hyp_2^2*((eccentricity_hyp^2)-1);
b_square2:decimalplaces(vertices_hyp_2^2*((eccentricity_hyp^2)-1),2);
b_square3:round(vertices_hyp_2^2*((eccentricity_hyp^2)-1));   
b_square4:is(abs(b_square3-b_square2)=0.0); 
b_square: if b_square4=true then b_square3 else b_square2;
simp: false;
standard_form: (x^2/(vertices_hyp_2^2))-(y^2/b_square)=1;]]></text>
