 var b = brd.create('slider', [[1, -7], [6, -7],[{#b_left#},{#b_cor#},{#b_right#}]], { name: 'b', snapWidth: {#snap_b#} });
        var c = brd.create('slider', [[1, -6], [6, -6],[{#c_left#},{#c_cor#},{#c_right#}]], { name: 'c', snapWidth: {#snap_c#} });
       var d = brd.create('slider', [[-7, -5], [-2, -5],[{#d_left#},{#d_cor#},{#d_right#}]], { name: 'd', snapWidth: {#snap_d#} });
        var f = brd.create('slider', [[-7, -6], [-2, -6],[{#f_left#},{#f_cor#},{#f_right#}]], { name: 'f', snapWidth: {#snap_f#} });

        var func1 = brd.create('functiongraph', [function (x) {
