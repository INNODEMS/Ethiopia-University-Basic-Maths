    <questionvariables>
      <text>/*Defining the equation of the line and the equation of the circle*/
a:rand(5)+4;
b:-1*(a-rand(3));
c:-1*(a+rand(5));
h:rand_with_step(-5,5,1);
k:rand_with_step(-5,5,1);
r:a;
eq_line: a*x + b*y + c = 0;
eq_circle: (x - h)^2 + (y - k)^2 = r^2;
yy:rhs(solve(eq_line, y)[1]);
xx:rhs(solve(eq_line, x)[1]);
expr1: (x - h)^2 + (yy- k)^2 = r^2;
expr2:simplify(expr1);
expr2a:lhs(expr2);
expr2b:expr2a*denom(expr2a)-rhs(expr2)*denom(expr2a)=0;
expr3:simplify(expr2b); /*quadratic equation*/
expr4:factor(expr3)/ r^2;
x_vals:solve(expr4);
x_1:rhs(x_vals[1]);
x_2:rhs(x_vals[2]);
y_1:subst(x_1,x,yy);
y_2:subst(x_2,x,yy);
point_p:[x_1,y_1];/*points of intersection*/
point_q:[x_2,y_2];
/*Model answers*/
ta1:yy;
ta2: (yy- k)^2;
ta3:expr3;
ta4:x_1;
ta5:x_2;
ta6:y_1;
ta7:y_2;
ta8:point_p;
ta9:point_q;</text>
