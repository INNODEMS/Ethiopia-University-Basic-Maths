    <questionvariables>
      <text>/*Defining the randomized points*/
P_x: rand_with_step(-5,5,1);
P_y:  rand_with_step(-6,5,1);
P_z:  rand_with_step(-4,7,1);
Q_x:  rand_with_prohib(-7,7,[P_x]);
Q_y:  rand_with_prohib(-5,5,[P_y]);
Q_z:  rand_with_prohib(-4,4,[P_z]);
PQ_x: Q_x-P_x; /*abscissa of vector PQ*/
PQ_y: Q_y-P_y; /*ordinate of vector PQ*/
PQ_z: Q_z-P_z; /*cote of vector PQ*/

/*Model answers*/
ta_1: PQ_x;
ta_2: PQ_y;
ta_3: PQ_z;
ta_4: ta_1*alpha+P_x;
ta_5: ta_2*alpha+P_y;
ta_6: ta_3*alpha+P_z;
equation1: (x -P_x)/ta_1;
equation2: (y -P_y)/ta_2;
equation3: (z -P_z)/ta_3;
ta_7: equation1;
ta_8: equation2;
ta_9: equation3;</text>
    </questionvariables>
