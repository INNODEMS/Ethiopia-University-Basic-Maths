    <questionvariables>
      <text>/* Starting from the answer- rref matrix; solution matrix */
ans_x:rand(10)-5;
ans_y:rand(10)-5;
ans_z:rand(10)-5;
rrefA: matrix(
    [1,0,0,ans_x],
    [0,1,0,ans_y],
    [0,0,1,ans_z]
);
ta2:rrefA;

/* ----- FUNCTIONS*/
/* multiplies a specific row in the given matrix by a constant value. */
row_times_const(matrix, row_no, const) := block(
    A:copymatrix(matrix),
    A[row_no]: const*A[row_no],
    return(A)
);

/* swaps two rows in the given matrix; row_no1 and row_no2 in matrix */
swap_rows(matrix, row_no1, row_no2) := block(
    A:copymatrix(matrix),
    A[row_no1]: matrix[row_no2],
    A[row_no2]: matrix[row_no1],
    return(A)
);

/* Add a multiple of one row to another row in the matrix.*/
sum_rows(matrix, row_no1, row_no2, const) := block(
    A:copymatrix(matrix),
    A[row_no1]: matrix[row_no1] + const*matrix[row_no2],
    return(A)
);
/* ----- END FUNCTIONS */

/* manipulation constants */
mult_const: random_permutation([rand(2)+2, rand(2)+2, rand(2)+2]);
add_const: append(
    random_permutation([1,1,rand(3)+1,rand(4)+1]), 
    [rand(3)+1, rand(3)+1]
);

/* Going from the solution matrix in RREF to the question matrix */
A1: sum_rows(rrefA, 2, 3, add_const[1]);
A2: sum_rows(A1, 1, 3, add_const[2]);
A3: row_times_const(A2, 3, mult_const[1]);

A3: sum_rows(A3, 3, 2, add_const[3]);
A4: sum_rows(A, 1, 2, add_const[4]);
A5: row_times_const(A4, 2, mult_const[2]);

A6: sum_rows(A5, 3, 1, add_const[5]);
A7: sum_rows(A6, 2, 1, add_const[6]);
A8: row_times_const(A7, 1, mult_const[3]);
ta1:A8;

/* From the question matrix, undoing each row operation to get to the RREF */
A9: row_times_const(A8, 1, 1/mult_const[3]); /*step 1*/
A10: sum_rows(A9, 2, 1, -add_const[6]); /*step 2*/
A11: sum_rows(A10, 3, 1, -add_const[5]); /*step 3*/

A12: row_times_const(A11, 2, 1/mult_const[2]); /*step 4*/
A13: sum_rows(A12, 1, 2, -add_const[4]); /*step 5*/
A14: sum_rows(A13, 3, 2, -add_const[3]); /*step 6*/

A15: row_times_const(A14, 3, 1/mult_const[1]); /*step 7*/
A16: sum_rows(A15, 1, 3, -add_const[2]); /*step 8*/
A17: sum_rows(A16, 2, 3, -add_const[1]); /*step 9*/

A18:  matrix(
    [ans_x],
    [ans_y],
    [ans_z]); /*Isolate the last column*/</text>
